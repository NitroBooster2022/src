<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <param name="robot_description" command="$(find xacro)/xacro $(find urdf02_gazebo)/urdf/car.urdf.xacro" />
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find urdf02_gazebo)/worlds/box_house.world"/>
        <!-- <arg name="world_name" value="$(find urdf02_gazebo)/worlds/mcgill.world"/> -->
        <!-- <arg name="world_name" value="$(find urdf02_gazebo)/worlds/world_with_separators.world"/> -->
        <arg name="paused"       value="false"/>    
        <arg name="use_sim_time" value="true"/>
        <arg name="debug"        value="false"/>
        <arg name="gui"          value="true"/>
    </include>
    <node pkg="gazebo_ros" type="spawn_model" name="spawn_model" args="-urdf -model car -param robot_description "/>
    <node pkg="joint_state_publisher" name="joint_state_publisher" type="joint_state_publisher" />
    <node pkg="robot_state_publisher" name="robot_state_publisher" type="robot_state_publisher" />
</launch>